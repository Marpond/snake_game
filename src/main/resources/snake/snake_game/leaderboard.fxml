<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" styleClass="gridbg" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="snake.snake_game.LeaderboardController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToMenu" text="&lt;" textFill="WHITE">
         <styleClass>
            <String fx:value="font" />
            <String fx:value="blackbg" />
         </styleClass></Button>
      <TableView fx:id="table" layoutX="58.0" layoutY="69.0" prefHeight="519.0" prefWidth="480.0" style="-fx-font-size: 10;" stylesheets="@style.css">
        <columns>
          <TableColumn fx:id="date" prefWidth="263.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Date" />
          <TableColumn fx:id="name" prefWidth="160.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Name" />
            <TableColumn fx:id="score" prefWidth="56.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Score" />
        </columns>
         <styleClass>
            <String fx:value="blackbg" />
            <String fx:value="font" />
         </styleClass>
      </TableView>
   </children>
   <effect>
      <SepiaTone level="0.2" />
   </effect>
</AnchorPane>
