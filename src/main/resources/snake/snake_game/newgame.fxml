<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" styleClass="gridbg" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="snake.snake_game.NewGameController">
   <TextField fx:id="usernameTextField" alignment="CENTER" layoutX="182.0" layoutY="160.0" prefHeight="29.0" prefWidth="236.0" promptText="enter username" style="-fx-font-size: 14;">
      <styleClass>
         <String fx:value="blackbg" />
         <String fx:value="font" />
      </styleClass></TextField>
   <Button fx:id="startBtn" layoutX="221.0" layoutY="278.0" mnemonicParsing="false" onAction="#switchToGame" text="Start">
      <styleClass>
         <String fx:value="font" />
         <String fx:value="blackbg" />
      </styleClass></Button>
   <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToMenu" text="&lt;">
      <styleClass>
         <String fx:value="font" />
         <String fx:value="blackbg" />
      </styleClass></Button>
   <CheckBox fx:id="cursedCheck" contentDisplay="TEXT_ONLY" layoutX="14.0" layoutY="568.0" mnemonicParsing="false">
      <cursor>
         <Cursor fx:constant="DEFAULT" />
      </cursor></CheckBox>
   <effect>
      <SepiaTone level="0.2" />
   </effect>
</AnchorPane>
